  <!-- Vet Finder Section -->
  <section id="findersec">
    <div class="container-fluid">
      <div class="row">
        <div class="container">
          <div class="row">
            <input type="hidden" id="data-input" value="">
            <h3 class="modal-title"><span class="fa fa-stethoscope"></span> Vet Locations</h3>
          </div>
          <div class="row">
            <form class="form-inline">
              <div class="form-group">
                <div class="input-group">
                  <div class="input-group-addon"><span class="fa fa-map-signs"></span></div>
                  <input type="text" class="form-control" id="addresstext" placeholder="Address">
                </div>
                <button id="findlocation" type="button" class="btn btn-primary"><span class="fa fa-map-marker" title="Find on map"></span></button>
                <button id="recenter" type="button" class="btn btn-primary" title="Recenter"><span class="fa fa-dot-circle-o"></span></button>
                <button id="loc-confirm" type="button" class="btn btn-primary" title="Confirm location"><span class="fa fa-check"></span></button>
              </div>
            </form>
          </div>
          <div class="row">
            <div id="mapinfo">
              <p id="address"></p>
              <p>
                <font>Lat: <span id="baselat"></span>&nbsp;, &nbsp;Long: <span id="baselng"></span></font>
              </p>
